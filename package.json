    "@vitest/ui": "^2.1.6",
    "eslint": "^9.29.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.2.0",
    "jest": "^30.0.3",
    "jsdom": "^25.0.1",
    "semver": "^7.7.2",
    "sharp": "^0.34.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.1",
