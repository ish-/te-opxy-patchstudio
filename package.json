{
  "name": "op-patchstudio",
  "private": true,
  "version": "0.14.6",
  "type": "module",
  "description": "Free & open source preset creator for OP synthesizers. Upload samples, edit waveforms, adjust settingsand generate patches instantly.",
  "repository": {
    "type": "git",
    "url": "https://github.com/joseph-holland/op-patchstudio.git"
  },
  "homepage": "https://op-patch.studio",
